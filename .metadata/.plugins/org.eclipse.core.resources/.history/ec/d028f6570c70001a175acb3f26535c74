	<script type="text/javascript">

          $(document).ready(function(){
          $.ajax({
           url: "/client-space-service/myDeposits",
           type: 'get',
           dataType: 'json',
           contentType: 'application/json',
           cache:false,
           timeout:600000,
     
           success: function (response) {              
           var total=0 ;
           $.each(response, function (i, item) {
        	   total +=item.amount;
    
        });
  $('#deposit').append(total);

    }
    });


          $.ajax({
              url: "/client-space-service/myWithdrawals",
              type: 'get',
              dataType: 'json',
              contentType: 'application/json',
              cache:false,
              timeout:600000,
        
              success: function (response) {              
            	  var total=0 ;
                  $.each(response, function (i, item) {
               	   total +=item.amount;
           
               });
         $('#withdrawal').append(total);
           }
       });




          $.ajax({
              url: "/client-space-service/myTransferts",
              type: 'get',
              dataType: 'json',
              contentType: 'application/json',
              cache:false,
              timeout:600000,
        
              success: function (response) {              
            	  var total=0 ;
                  $.each(response, function (i, item) {
               	   total +=item.amount;
           
               });
         $('#transfert').append(total);
           }
       });

       
});

        
     
</script>

